  "components": {
    "schemas": {
      "GetCviOracleHistoryDataQueryDto": {
        "type": "object",
        "properties": {
          "fromBlockTimestamp": {
            "type": "number"
          },
          "toBlockTimestamp": {
            "type": "number"
          },
          "groupBy": {
            "type": "string",
            "enum": [
              "day",
              "hour",
              "minutes"
            ]
          }
        },
        "required": [
          "groupBy"
        ]
      },
      "CviOracleIndexValueArgsTypeDto": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "number"
          },
          "value": {
            "type": "number"
          }
        },
        "required": [
          "timestamp",
          "value"
        ]
      },
      "MinimalCviOracleEventDto": {
        "type": "object",
        "properties": {
          "events": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CviOracleIndexValueArgsTypeDto"
            }
          },
          "lastBlockTimestamp": {
            "type": "number",
            "nullable": true
          }
        },
        "required": [
          "events",
          "lastBlockTimestamp"
        ]
      },
      "MinimalCviOracleEventOhlcDto": {
        "type": "object",
        "properties": {
          "events": {
            "items": {
              "type": "array"
            },
            "type": "array"
          },
          "lastBlockTimestamp": {
            "type": "number",
            "nullable": true
          }
        },
        "required": [
          "events",
          "lastBlockTimestamp"
        ]
      },
      "CviOracleLatestDto": {
        "type": "object",
        "properties": {
          "cviIndex": {
            "type": "number"
          },
          "timestamp": {
            "type": "number"
          }
        },
        "required": [
          "cviIndex",
          "timestamp"
        ]
      }
    }
  }
