{
  "name": "beta-cvi-ui",
  "private": true,
  "ui": true,
  "version": "1.0.0",
  "scripts": {
    "start:dev:cvi": "rm -rf index.html && cp index_cvi.html index.html && cross-env INIT_WEBSITE=CVI vite",
    "start:dev:armadillo": "rm -rf index.html && cp index_armadillo.html index.html && cross-env INIT_WEBSITE=ARMADILLO vite",
    "bundle:base": "rm -rf ui-dist && cross-env NODE_OPTIONS=--max_old_space_size=10240 vite build && rimraf ui-dist/**/*.js.map",
    "bundle:armadillo": "rm -rf index.html && cp index_armadillo.html index.html && INIT_WEBSITE=ARMADILLO yarn bundle:base && rm -rf index.html",
    "bundle:cvi": "rm -rf index.html && cp index_cvi.html index.html && INIT_WEBSITE=CVI yarn bundle:base && rm -rf index.html",
    "serve": "serve -s ui-dist",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
