    "solidity-compile": "yarn workspace @coti-cvi/contracts-deploy run compile-all",
    "solidity-tests": "yarn workspace @coti-cvi/contracts-deploy run test-all",
    "prettier:all": "yarn prettier --write .",
    "ui-tests": "vitest run packages/hardhat-csm-ui",
    "test": "jest --force-exit",
    "clean": "rm -r yarn-error.log tsconfig.tsbuildinfo dist packages/**/*dist > /dev/null 2>&1 || exit 0",
    "find-any-bad-import": "ts-node --swc -r tsconfig-paths/register find-any-bad-import.ts",
    "build": "ts-node --swc -r tsconfig-paths/register split-root-dist-to-packages.ts",
    "build:watch": "yarn tsc --watch",
    "contracts-deploy:start-node:in-docker": "yarn workspace @coti-cvi/contracts-deploy run start:hardhat:node:base --port 9545 --hostname 0.0.0.0",
