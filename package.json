    "dev-resources:down": "docker-compose -f dev.docker-compose.yml down --remove-orphans",
    "dev-resources:pull": "docker-compose -f dev.docker-compose.yml pull",
    "dev-resources:reset": "yarn dev-resources:down && yarn dev-resources:pull && yarn dev-resources:up"
