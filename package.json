    "start:traders-ui": "yarn workspace traders-ui run start:dev",
    "start:hardhat-csm-ui": "yarn workspace hardhat-csm-ui run start:dev",
    "start:cvi-tweet": "yarn workspace cvi-tweet run start:dev",
    "start:chatgpt-server": "yarn workspace chatgpt-server run start:dev",
    "start:chatgpt-server-wrapper": "yarn workspace chatgpt-server-wrapper run start:dev",
    "start:data-feed": "yarn workspace data-feed run start:dev",
