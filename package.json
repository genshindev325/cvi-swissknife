  "scripts": {
    "start-arb:non-technical": "git reset head --hard && git fetch && git checkout origin/main && rm -rf packages/api packages/cvi-monitor && yarn && yarn start:arb-local",
    "tunnle:9091": "ngrok http 9091",
    "create-types-from-common-abis": "yarn typechain --target ethers-v5 --out-dir packages/auto-generated-code/src/common-abi-types 'packages/lw-sdk/src/common-abis/**/*.json'",
    "kill-ports": "yarn scripts-cli kill-used-dev-ports --ports",
    "compilations-headquarters": "newsh --split 'yarn workspace @coti-cvi/contracts-deploy start:polygon-local' && newsh --split-horizontally 'yarn create-auto-generated-code:watch' && yarn build:watch",
    "create-auto-generated-code": "yarn workspace scripts-cli start create-auto-generated-code",
    "create-auto-generated-code:watch": "yarn create-auto-generated-code --watch",
    "aws-code-build-logs:build-id": "yarn scripts-cli aws-code-build-logs --build-id",
    "aws-code-build-logs:current-branch": "yarn scripts-cli aws-code-build-logs --current-branch",
    "reset-cvi-k8s-resources:prod": "yarn workspace helm-chart run reset-cvi-k8s-resources:prod",
    "uninstall-all-cvi-k8s-resources:prod": "yarn workspace helm-chart run uninstall-all-cvi-k8s-resources:prod",
    "install-all-cvi-k8s-resources:prod": "yarn workspace helm-chart run install-all-cvi-k8s-resources:prod",
    "load-k8s-config": "aws eks --region eu-west-1 update-kubeconfig --name prd-01-eks-cvi",
    "open-cvi-db-ssh-tunnle": "ssh -L 3307:prd-01-db-cvi.cbeazsamcoyd.eu-west-1.rds.amazonaws.com:3306 prd-01-db-cvi -N",
    "share-terminal-tab1": "tty-share --public --listen localhost:8001",
    "share-terminal-tab2": "tty-share --public --listen localhost:8002",
    "postinstall": "patch-package && husky install",
    "lint:code:base": "eslint --cache --cache-location eslint-cache/cache --max-warnings 0",
    "lint:code": "yarn lint:code:base 'packages/*/src/**/*.{ts,d.ts,tsx}'",
    "lint:versions": "syncpack list-mismatches",
    "scripts-cli": "yarn workspace scripts-cli run start",
    "scripts-cli:run-ci": "yarn scripts-cli run-ci",
    "premium-coeff-tests": "yarn workspace py-parabola-coefficients run test",
